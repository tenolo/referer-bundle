{
    "name": "tenolo/referer-bundle",
    "description": "Provides simple functions to use and set referer.",
    "keywords": [
        "referer",
        "tenolo",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo OHG",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0",
        "league/uri-manipulations": "^1.5",
        "league/uri-schemes": "^1.2",
        "psr/http-message": "^1.0",
        "twig/twig": "^1.37|^2.6"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\RefererBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
